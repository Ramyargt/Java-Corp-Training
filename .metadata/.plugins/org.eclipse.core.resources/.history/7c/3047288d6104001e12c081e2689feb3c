package com.rgt.training.session3;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class Car extends Vehicle{

	Scanner sc= new Scanner(System.in);
	List<String>vehicledetails; 
	
	public void addVehicleDetails(String license, String make, String model) {
		
		setLicensePlate(license);
		setMake(make);
		setModel(model);
	}
	
	public String[] getVehicleDetails() {
		
		System.out.println("licensePlate is :"+getLicensePlate());
		System.out.println("make of vehicle is :"+getMake());
		System.out.println("model of vehicle is :"+getModel());
	//	String[] details= new String[getLicensePlate(),getMake(),getModel()];
		return null;
		
	}
}
